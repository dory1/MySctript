echo "</td><td>"
sshpass -p 'V,jkv&Jsav$' ssh dory@$server.bezeqint.net cat /tmp/serFailcheck.log 2> /dev/null
echo "</td></tr>"
done
echo "</table>

</body>
</html>
"

